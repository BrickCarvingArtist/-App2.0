doctype
html
	head
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no")
		each i in style
			link(rel="stylesheet", href=i)
		title!= title
	body!= page
		script(src="/lib/jquery-2.1.4.min.js")
		script(src="/lib/react.js")
		script(src="/lib/react-router.min.js")
		script(src="/lib/redux.min.js")
		each i in script
			script(src=i)